#include<bits/stdc++.h>
using namespace std;
void reverse_array(int arr[],int k,int n)
{
    reverse(arr,arr+k);
    reverse(arr+k,arr+n);
    reverse(arr,arr+n);
    for(int i=0;i<n;i++)
    {
    cout<<arr[i]<<" ";    
    
    }
    
}
int main()
{
    int arr[]={1,3,5,7,9};
    int k=4;
    int n=sizeof(arr)/sizeof(int);
    reverse_array(arr,k,n);
    return 0;
}
